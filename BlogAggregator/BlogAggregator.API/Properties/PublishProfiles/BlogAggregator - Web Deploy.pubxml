<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://blogaggregator.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <PrecompileBeforePublish>True</PrecompileBeforePublish>
    <EnableUpdateable>True</EnableUpdateable>
    <DebugSymbols>False</DebugSymbols>
    <WDPMergeOption>DonotMerge</WDPMergeOption>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>blogaggregator.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>BlogAggregator</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$BlogAggregator</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="BlogAggregator_oauth" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:originstudents.database.windows.net,1433;Initial Catalog=BlogAggregator_oauth;User ID=azure@originstudents;Password=OCA_azur3" Name="Data Source=tcp:originstudents.database.windows.net,1433;Initial Catalog=BlogAggregator_oauth;User Id=azure@originstudents;Password=OCA_azur3" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="BlogAggregator.Data.Infrastructure.BlogAggregatorDbContext, BlogAggregator.Data" MigrationConfiguration="BlogAggregator.Data.Migrations.Configuration, BlogAggregator.Data" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)BlogAggregator_oauth-Web.config Connection String">
      <ParameterValue>Data Source=tcp:originstudents.database.windows.net,1433;Initial Catalog=BlogAggregator_oauth;User Id=azure@originstudents;Password=OCA_azur3</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="BlogAggregator_oauth" />
  </ItemGroup>
</Project>